{
  "name": "a-responsive-react",
  "version": "3.0.0",
  "description": "Responsive containers for React",
  "main": "entry/entry.js",
  "scripts": {
    "start": "npm run open",
    "open": "webpack-dev-server --env.mode development --config ./examples/@src/webpack.config.js --open",
    "prepare-modules": "webpack --config ./examples/modules/webpack.config.js --env.mode development",
    "build-modules": "webpack --config ./examples/modules/webpack.config.js --env.mode production",
    "build-examples": "npm run build-modules && webpack --config ./examples/@src/webpack.config.js --env.mode production"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/andrey-brill/a-responsive-react"
  },
  "author": "Andrey Brill <andrey@a-brill.com> (https://github.com/andrey-brill)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andrey-brill/a-responsive-react/issues"
  },
  "homepage": "https://github.com/andrey-brill/a-responsive-react",
  "keywords": [
    "responsive",
    "containers",
    "pixel",
    "react"
  ],
  "devDependencies": {
    "a-webpack-configs": "^2.1.2"
  },
  "dependencies": {
    "a-bundle": "^1.0.3",
    "a-responsive-containers": "^3.2.1",
    "react": "^16.13.1"
  }
}
